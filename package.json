{
  "name": "psyreply2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/psyreply2/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./main/jest-e2e.json",
    "deploy": "sh uploadProd.sh",
    "deploy-test": "sh uploadTestProd.sh"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/websockets": "^9.2.1",
    "@types/iconv": "^3.0.1",
    "@types/multer": "^1.4.7",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "iconv-lite": "^0.6.3",
    "iso-8859-2": "^3.0.4",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "object.assign": "^4.1.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.25.3",
    "sequelize-cli": "^6.5.2",
    "sequelize-typescript": "^2.1.5",
    "socketio": "^1.0.0",
    "telegraf-ts": "^3.89.1",
    "windows-1251": "^3.0.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "object-assign": "^4.1.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/db-models(|/.*)$": "<rootDir>/libs/db-models/src/$1",
      "^@app/models(|/.*)$": "<rootDir>/libs/models/src/$1",
      "^@app/dtos(|/.*)$": "<rootDir>/libs/dtos/src/$1",
      "^@app/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/exceptions(|/.*)$": "<rootDir>/libs/exceptions/src/$1",
      "^@app/filters(|/.*)$": "<rootDir>/libs/filters/src/$1",
      "^@app/guards(|/.*)$": "<rootDir>/libs/guards/src/$1",
      "^@app/strategies(|/.*)$": "<rootDir>/libs/strategies/src/$1",
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@utils/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@app/application(|/.*)$": "<rootDir>/libs/application/src/$1"
    }
  }
}